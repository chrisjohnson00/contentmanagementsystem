{% if settings|length >0 %}
    {% set setting = settings|first %}
    <script src="http://code.jquery.com/jquery-{{ setting.jqueryVersion }}.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/{{ setting.bootstrapVersion }}/js/bootstrap.min.js"></script>
{% else %}
    <script src="http://code.jquery.com/jquery-2.2.0.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
{% endif %}