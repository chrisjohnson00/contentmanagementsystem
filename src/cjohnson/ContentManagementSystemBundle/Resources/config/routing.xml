<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <import resource="@cjohnsonContentManagementSystemBundle/Resources/config/routing.yml"/>
    <route id="cjohnson_content_management_system_dbroute" path="/{uri}">
        <default key="_controller">cjohnsonContentManagementSystemBundle:Default:index</default>
    </route>
    <route id="cjohnson_content_management_system_homepage" path="/">
        <default key="_controller">cjohnsonContentManagementSystemBundle:Default:home</default>
    </route>

</routes>
